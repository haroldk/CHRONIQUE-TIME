/* ------------------------------------------------------------------------------------------------*/
/*                                    CHRONIQUE RESET                                              */
/* ------------------------------------------------------------------------------------------------*/

body{
	
	margin:0; width:100%; 	
	background-color: #ebebeb;
	font-family: Verdana, Geneva, sans-serif, "Arial Narrow", Arial, sans-serif;
	margin: 0 auto;	
	
}

ul, li, h1,h2,h3,p{ margin:0; padding:0;}

img{ border: 0;}

dl{ margin: 0;}

body a{ text-decoration: none; }
body a:HOVER{ text-decoration: none; }

.errors{ display:none; }

 hr { border: 0; border-top: 3px solid black; width: 780px; margin: 5px auto; -moz-border-top: 3px solid black;}


