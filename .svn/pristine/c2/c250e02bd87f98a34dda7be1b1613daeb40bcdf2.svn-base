	height: 40px;
	overflow: hidden;
}
.index-chro .index-chro-right .chro-comm {
	color: #e5a0df;
	font-size: 12px;
	font-style: italic;
	position: absolute;
	bottom: 0px;	
}
.note-etoiles {
	
	position:absolute;
	top: 12px;
	right: 0px;	
}
a.lire-chronique {
	
	display: : block;
	position: absolute;
	bottom: 0px;	
	right: 0px;
	color:#0077af;
	font-size: 11px;
	font-style: italic;
}

#allchro {
	
	margin-right: 10px;
	text-align: right;
	font-size: 12px;
	color:black;
	
}
#allchro a {
	
	color:black;
	font-weight: bold;
}
#allchro a:HOVER { color:#0077af; }



#div-index #top-chro {
	
	width: 260px;
	float: left;
	text-align: center;
}
