}
.ie8 div#index-side #index-theme button, .ie8 #chronique-side #index-theme button { padding-top: 0px; }
.gecko div#index-side #index-theme button, .gecko #chronique-side #index-theme button {
	
	background: url('../img/general/nuage.png') no-repeat;
	width: 76px;
	height: 38px;
	border: 0px;
	color: white;
	margin: 10px 20px;
	font-style: italic;
	padding-top: 6px;
	text-align: left;
	padding-left: 36px;
	cursor: pointer;
	
}
div#index-side #index-theme button a:HOVER, #chronique-side #index-theme button a:HOVER {
	color: black;
}	
	
div#index-side #index-theme button a, #chronique-side #index-theme button a {
	
	display: block;
	width: 76px;
	height: 38px;
	color: white;
	cursor: pointer;
}


div#index-side #index-fb {

	background-color: white;
	width: 100%;	
	margin-top: 5px;
}

div#wrapper div#index-side #index-connexion .index-insc-title {
	
	margin-top: 30px;
}

