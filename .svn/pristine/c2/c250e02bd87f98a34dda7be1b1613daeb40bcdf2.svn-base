#div-index .top-chronique p span {

	font-size: 12px;
}
#div-index .top-chronique p span.p1 a {

	color: black;
}
.top-chronique p span.p2 {

	color: #d649cb;
}


#index-conseil {
	
	width:235px; 
	padding-top: 20px;
	border-top: 1px solid white;
	position: absolute;
	right: 13px;
	top: 350px;
}
#index-conseil > img {
	
	text-align: center;
	margin-left: 12px;
}
#index-conseil img.img-conseil {
	
	margin-left: 22px;
}
#index-conseil > p {
	
	margin-top: 10px;
	font-size: 13px;	
}


div#index-side {
	
	width: 250px;
	position: absolute;
	right: 0px;
	top: 0px;
}
div#index-side #index-connexion {

	background-color: white;
	width: 100%;	
	height: 100%;	
	padding: 10px 0 1px;
}
div#index-side #index-connexion > p {

	margin: 10px 0;	
}
div#index-side #index-connexion form#Connexion dt {

	display: none;	
}
div#index-side #index-connexion form#Connexion dd {

	margin-left: 12px;
}
div#index-side #index-connexion form#Connexion input {

	width: 179px;
	height: 16px;
	color: white;
	margin-top: 5px;
	margin-left: 13px;
	font-style: italic;
	padding:7px 10px;background:#0077af;border:0px solid #bbbbbb;border-radius:10px;-moz-border-radius:10px;-webkit-border-radius:10px;
}
div#index-side #index-connexion form#Connexion input[type="submit"] {
	
	width: 197px;
	height: 34px;
	background: url("../img/general/btn_connexion.png") no-repeat center center;
	margin-top: 10px;
	color : transparent;
	cursor: pointer;
	border : none;
}

div#index-side #index-connexion p.btn-inscription {
	
	width: 197px;
	height: 34px;
	margin: 8px auto;
	cursor: pointer;
	background: url('../img/general/btn_inscription.png') no-repeat;

}
div#index-side #index-connexion p.btn-inscription a {

	width: 197px;
	height: 34px;
	display: block;	
}


div#index-side #index-theme, #chronique-side #index-theme {

	background-color: white;
	width: 100%;	
	height: 100%;	
	padding-top: 10px;
	margin-top: 5px;
}

div#index-side #index-theme button, #chronique-side #index-theme button {
	
	background: url('../img/general/nuage.png') no-repeat;
	width: 76px;
	height: 38px;
	border: 0px;
