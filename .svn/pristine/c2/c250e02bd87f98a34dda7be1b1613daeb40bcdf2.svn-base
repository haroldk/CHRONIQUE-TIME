@CHARSET "UTF-8";

/* ------------------------------------------------------------------------------------------------*/
/*                                    CHRONIQUE RESET                                              */
/* ------------------------------------------------------------------------------------------------*/

	ul, li, h1,h2,h3,p{ margin:0; padding:0;}
	
	img{ border: 0;}
	
	dl{ margin: 0;}
	
	a{ text-decoration: none; color: inherit;}
	
	.errors{ display:none; }

/* ------------------------------------------------------------------------------------------------*/
/*                                    CHRONIQUE GENERAL                                            */
/* ------------------------------------------------------------------------------------------------*/

body{
	
	margin:0; width:100%; 	
	font-family: Verdana, Geneva, sans-serif, "Arial Narrow", Arial, sans-serif;
	background-color: black;
	margin: 0 auto;	
	
}

#wrapper {
	
	width: 100%;
	margin: 0 auto;
	position:relative;
	overflow: hidden;
	background: url("../img/general/bg_body.jpg") repeat;
}
#wrapper > div {

	width: 1024px;
	margin: 0 auto;
	position:relative;
}

dt{
	
	display: block;
	width: 110px;
	padding: 0px 2px;
}

#dialog-connexion a.mdplost {

	float: right;
	text-decoration: underline;
	color: purple;
	font-style: italic;
}
/* ------------------------------------------------------------------------------------------------*/
/*                                    CHRONIQUE LAYOUT                                             */
/* ------------------------------------------------------------------------------------------------*/

header {

	width: 100%;
	height: 110px;
	background-image: url("../img/layout/bg_header.jpg");
	background-repeat: repeat-x;
	
	
}

header > div {
	
	width: 1024px;
	margin: 0 auto;
	padding-top: 25px;

}

header > div > img {
	
	margin-left: 30px;

}
.ie header > div > img {
	
	margin-left: 200px;

}
#slogan-head {
	
	left: 580px;
    position: absolute;
    top: 70px;
}

nav {
	
	width: 100%;
	height: 40px;
	background-image: url("../img/general/bg_body.jpg");
	background-repeat: repeat-x;		
}

nav ul {
	
	width: 1024px;
	margin: 0 auto;
	
}

nav ul li {
	
	width: auto;
	height: 40px;
	display: inline-block;
	*display: inline; zoom: 1;
	line-height: 40px;
	font-size: 10px;
	list-style-type: none;
	margin-left: 20px;
}
nav ul li a{
	
	color: black;
	font-size: 12px;
	font-weight: bold;
	font-style: italic;
	text-transform: capitalize;
}
nav ul li a:HOVER{
	
	color: grey;
}
nav ul li input[type="text"]{
	
	width: 185px;
	height: 13px;
	border : none;
	margin-top: 6px;
	margin-right: 10px;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	padding: 5px;
	padding-left: 10px;
	font-style: italic;
	font-size: 12px;
	color: grey;
	
}
nav ul li input[type="submit"]{
	
	width: 28px;
	height: 28px;
	background: url("../img/layout/btn_search.png") no-repeat center center;
	color : transparent;
	cursor: pointer;
	border : none;
}

footer {
	
	width: 100%;
	height: 185px;
	background: url("../img/layout/bg_footer.jpg") repeat-x;
		
}
footer > div {
	
	width: 1024px;
	height: 160px;
	margin: 0 auto;
	padding-top: 20px;
	position: relative;
}
footer > div div {
	
	width: 185px;
	height: 120px;
	position: relative;
	margin-right: 20px;
	float: left;
}
footer > div div H4{
	
	margin: 0;
	margin-bottom: 15px;
	font-weight: normal;
	color: whitesmoke;
	text-transform: uppercase;	
}

footer ul li {
	
	width: auto;
	line-height: 13px;
	list-style-type: none;
}
footer ul li a{
	
	color: silver;
	font-size: 11px;
}
footer ul li a:HOVER{
	
	color: white;
}
footer input[type="text"]{
	
	width: 165px;
	height: 13px;
	border : none;
	margin-right: 10px;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	padding: 5px;
	padding-left: 10px;
	font-style: italic;
	font-size: 12px;
	color: grey;
	
}
footer input[type="submit"]:HOVER{
	
	background: url("../img/layout/btn_valide_newsletter_hover.png") no-repeat center center;
}
footer input[type="submit"]{
	
	width: 73px;
	height: 23px;
	font-size: 12px;
	background: url("../img/layout/btn_valide_newsletter.png") no-repeat center center;
	margin-right: 6px;
	color : white;
	cursor: pointer;
	border : none;
	float: right;
}
footer > div div p {
	
	width: 270px;
	height: 100px;
	float: right;
	text-align: right;
	margin-right: 10px;
}
footer > div div p.social-logo {
	
	width: 115px;
}

footer > div div p > span.tweettime {
	
	color: #5bc7f5;
	text-transform: uppercase;
	font-size: 12px;
}
footer > div div p > span {
	
	color: white;
	font-size: 10px;
	display: block;
	width: 235px;
	height: 20px;
	overflow: hidden;
	margin: 0;
	margin-top: 5px;
	margin-left: 35px;
}
footer > div div.reso-soc {
	
	width: 405px;
	margin-right: 0px;
	
}
footer > div div.reso-soc p img {
	margin-top: 2px;	
}
footer > div div.reso-soc p img.icn_soc {
	margin-top: 5px;	
}
footer > div div.reso-soc p img.twitter_bird {
	
	position: absolute;
	left: 10px;
	top: 3px;
}
footer > div p.txt-footer2 {
	
	width: 1024px;
	height: 23px;
	margin: 0 auto;
	position: absolute;
	text-align: center;
	bottom: 0px;
	color: whitesmoke;
	font-size: 11PX;
}
footer > div p.txt-footer2 img{
	
	position: absolute;
	
}

