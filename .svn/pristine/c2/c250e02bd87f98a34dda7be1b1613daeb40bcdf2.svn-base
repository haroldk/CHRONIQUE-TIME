}

#div-index .top-chronique img.top-miniature {

	float: left;
	margin-left: 35px;
}
#div-index .top-chronique > img {

	position: absolute;
	top: 4px;
	left: 7px;
}
#div-index .top-chronique p {

	width: 175px;
	float: right;	
	margin-top: -2px;
	text-align: left;
	line-height: 14px;
