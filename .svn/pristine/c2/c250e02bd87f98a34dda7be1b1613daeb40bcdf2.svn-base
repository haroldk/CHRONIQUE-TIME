	height: 270px;
	position: relative;
	overflow: hidden;
	border-bottom: 1px solid black;
	
	background: #1178ad; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #1178ad 0%, #1178ad 50%, #1178ad 51%, #1178aa 81%, #0d679e 89%, #1e5799 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#1178ad), color-stop(50%,#1178ad), color-stop(51%,#1178ad), color-stop(81%,#1178aa), color-stop(89%,#0d679e), color-stop(100%,#1e5799)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #1178ad 0%,#1178ad 50%,#1178ad 51%,#1178aa 81%,#0d679e 89%,#1e5799 100%); /* Chrome10+,Safari5.1+ */
