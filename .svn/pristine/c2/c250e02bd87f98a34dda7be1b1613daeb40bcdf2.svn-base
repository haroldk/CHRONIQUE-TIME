}
#top-chronique > div {

	width: 1024px;
	height: 100%;
	margin: 0 auto;
	background: url('../img/index/fond_rose_chrojour.png') no-repeat ;
}
#top-chronique div div { float: left; }
#top-chronique div #top-img {
	
	width: 420px;
	height: 100%;
	background-color: blue;
		
}
#top-chronique div #top-right {
	
	width: 579px;
	height: 100%;
	margin-left: 25px;
	margin-top: 20px;
		
}
#top-chronique div #top-right p{
	
	color: white;	
	line-height: 27px;
}
#top-chronique div #top-right img {
	position: absolute;	
}

#top-chronique div #top-right .top-title {
	
	color: white;
