/*                                    CHRONIQUE INDEX                                              */
/* ------------------------------------------------------------------------------------------------*/


.slide1 {
	
	/*background: #0077af url("../img/index/bg_chronique.jpg") repeat-x;*/
	width: 100%;
	height: 360px;
	position: relative;
	border-bottom: 1px solid black;
	
	background: #1178ad; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #1178ad 0%, #1178ad 50%, #1178ad 51%, #1178aa 81%, #0d679e 89%, #1e5799 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#1178ad), color-stop(50%,#1178ad), color-stop(51%,#1178ad), color-stop(81%,#1178aa), color-stop(89%,#0d679e), color-stop(100%,#1e5799)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #1178ad 0%,#1178ad 50%,#1178ad 51%,#1178aa 81%,#0d679e 89%,#1e5799 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #1178ad 0%,#1178ad 50%,#1178ad 51%,#1178aa 81%,#0d679e 89%,#1e5799 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #1178ad 0%,#1178ad 50%,#1178ad 51%,#1178aa 81%,#0d679e 89%,#1e5799 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #1178ad 0%,#1178ad 50%,#1178ad 51%,#1178aa 81%,#0d679e 89%,#1e5799 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1178ad', endColorstr='#1e5799',GradientType=0 ); /* IE6-8 */
		
		
}
.slide1 > div {
	
	width: 1024px;
	height: 100%;
	margin: 0 auto;
	
}

#div-index {
	
	width: 750px;
	height: 660px;
	padding-top: 35px;
	position: relative;
			
}
#dern-chro {
	
	width: 485px;
	float: left;
	border-right: 1px solid white;			
}
.index-chro {
	
	width: 470px;
	height: 100px;
	margin: 20px 0;
}
.index-chro div {

	float: left;	
}

.index-chro > div > img {

	width: 105px;
	height: 90px;
	line-height: 100px;
	border: 3px solid #dddbdc;
}
.index-chro .index-chro-right {

	width: 345px;
	height: 95px;
